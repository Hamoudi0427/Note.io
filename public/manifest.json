{
    "name": "TO-Hacks",
    "description": "Build an Extension!",
    "version": "0.0",
    "manifest_version": 3,
    "action":{
        "default_popup":"popup.html"
    },
    "content_scripts":[{
        "matches":[
            "<all_urls>"
        ],
        "js":["./dist/content.js"],
        "all_frames":false
    }],
    "background":{
        "service_worker":"./dist/background.js"
    }
  }